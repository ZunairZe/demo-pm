<div class='card'>
  <div class="card-header">
    <h6><b class='text-success'>Overall View of the Projects</b></h6>
  </div>
  <div class='card-body'>
    <div class="row">
      <div class="col-6">
        <%= pie_chart @projects.group(:starting_date).count %>
      </div>
      <div class="col-6">
        <%= line_chart @projects.group(:status).count, xtitle: "Status", ytitle: "Count" %>
      </div>
    </div>
  </div>
</div><hr>

<div class='card-body'>
  <h3>Projects Created By You</h3>
  <%= render "shared/search", url: projects_path %>
  <%= render 'projects' %>
</div>
